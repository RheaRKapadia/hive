{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "github": {
      "enabled": true,
      "silent": true
    },
    "git": {
      "deploymentEnabled": {
        "verceltry": true
      }
    }
  }
